generator client {
  provider = "prisma-client-js"
}

datasource db {
  //  Do not use sqlite in production
  provider = "sqlite"
  url = "file:.dev.db"
  // url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
